CREATE TABLE IF NOT EXISTS O2PLUGIN_SHOP_OBJ_TRANSACTION_LOG (
  objectId         int(11)      NOT NULL default '0',
  clientIdentifier varchar(255) NOT NULL default '',
  type             varchar(32)  NOT NULL default '',
  message          longtext     NOT NULL,
  epochTime        int(11)      NOT NULL default '0',
  KEY objectIdIndex (objectId)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 DEFAULT COLLATE=utf8_danish_ci;
